{
    "extends": [
        "eslint:recommended",
        "google"
    ],
    "rules": {
        "no-console": "error",
        "no-debugger": "error",
        "comma-dangle": [
            "error",
            {
                "objects": "never"
            }
        ],
        "max-len": [
            "warn",
            {
                "code": 100
            }
        ],
        "indent": "off",
        "no-tabs": "off",
        "no-mixed-spaces-and-tabs": "off",
        "space-before-function-paren": "off",
        "padded-blocks": "off",
        "require-jsdoc": "off",
        "quotes": "off",
        "no-var": "off",
        "spaced-comment": "off",
        "key-spacing": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "env": {
        "es6": true,
        "browser": true,
        "amd": true
    },
    "globals": {
        "angular": true,
        "$": true,
        "_": true,
        "d3": true,
        "getError": true
    }
}
