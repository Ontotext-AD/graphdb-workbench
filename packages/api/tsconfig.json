{
  "extends": "ts-config-single-spa",
  "files": ["src/ontotext-workbench-api.ts"],
  "compilerOptions": {
    "declarationDir": "dist",
    "outDir": "dist",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types"],
      "target": "ESNext",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["src/**/*.test*"]
}
