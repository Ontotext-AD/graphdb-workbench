{
  "name": "shared-components",
  "version": "0.0.1",
  "description": "Shared components for the GraphDB Workbench application",
  "license": "MIT",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/shared-components/shared-components.esm.js",
  "exports": {
    ".": {
      "import": "./dist/shared-components/shared-components.esm.js",
      "require": "./dist/shared-components/shared-components.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build:watch": "stencil build --watch",
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "npm run test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "docker compose --ansi never up --exit-code-from cypress",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "lint:fix": "eslint --fix src/**/*{.ts,.tsx}",
    "stylelint": "stylelint \"src/**/*.{css,scss}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix",
    "sonar": "./sonar-run.sh"
  },
  "devDependencies": {
    "@stencil-community/eslint-plugin": "^0.10.0",
    "@stencil/core": "^4.35.1",
    "@stencil/sass": "^3.2.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.4",
    "@typescript-eslint/parser": "^8.35.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-react": "^7.37.5",
    "jest": "^30.0.3",
    "jest-cli": "^30.0.3",
    "jest-stencil-runner": "^0.0.6",
    "puppeteer": "^24.11.0",
    "sonarqube-scanner": "^3.3.0",
    "stylelint": "^16.23.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "stylelint-scss": "^6.12.1",
    "ts-jest": "^29.4.0"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.7.4",
    "single-spa": "^6.0.3"
  }
}
