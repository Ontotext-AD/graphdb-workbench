{
  "name": "shared-components",
  "version": "0.0.1",
  "description": "Shared components for the GraphDB Workbench application",
  "license": "MIT",
  "main": "components-dist/index.cjs.js",
  "module": "components-dist/index.js",
  "types": "components-dist/types/index.d.ts",
  "collection": "components-dist/collection/collection-manifest.json",
  "collection:main": "components-dist/collection/index.js",
  "unpkg": "components-dist/shared-components/shared-components.esm.js",
  "exports": {
    ".": {
      "import": "./components-dist/shared-components/shared-components.esm.js",
      "require": "./components-dist/shared-components/shared-components.cjs.js"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "components-dist/",
    "loader/"
  ],
  "scripts": {
    "build:watch": "stencil build --watch",
    "prebuild": "stencil build",
    "build": "webpack --mode production",
    "start": "webpack serve --mode development --port 9005",
    "start:standalone": "stencil build --dev --watch --serve",
    "test": "npm run test:unit",
    "test:unit": "stencil test --spec",
    "test:unit:watch": "stencil test --spec --watchAll",
    "test:e2e": "stencil test --e2e",
    "test:e2e:watch": "stencil test --e2e --watchAll",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "docker-compose up --exit-code-from cypress",
    "generate": "stencil generate",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "lint:fix": "eslint --fix src/**/*{.ts,.tsx}",
    "sonar": "./sonar-run.sh"
  },
  "devDependencies": {
    "@stencil-community/eslint-plugin": "^0.9.0",
    "@stencil/core": "^4.23.0",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^22.10.2",
    "@typescript-eslint/parser": "^8.18.0",
    "concurrently": "^9.1.0",
    "cypress": "^13.17.0",
    "eslint-plugin-react": "^7.37.2",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^23.10.4",
    "sonarqube-scanner": "^3.3.0",
    "ts-jest": "^29.2.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-config-single-spa": "^7.0.0",
    "webpack-dev-server": "^5.2.0"
  },
  "dependencies": {
    "font-awesome": "^4.7.0",
    "single-spa": "^6.0.1",
    "tippy.js": "^6.3.7"
  }
}
